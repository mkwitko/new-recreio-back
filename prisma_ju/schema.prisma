    qua_idquadra        Int      @id @default(autoincrement())
    qua_associado       Int
    qua_sequencia       Int
    qua_dtlancamento    DateTime
    qua_local           Int
    qua_servico         Int
    qua_dtreserva       DateTime
    qua_idhorario       Int
    qua_qtetickets      Int
    qua_vlrquadra       Int
    qua_vlrtotal        Int
    qua_vlriluminacao   Int
    qua_compareceu      String
    qua_cancelada       String
    qua_usuariocancelou String
    qua_gerardebito     String
